{
    "name": "automagistre/automagistre",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.2",
        "csa/guzzle-bundle": "^3.0.1",
        "doctrine/doctrine-bundle": "^1.9.0",
        "doctrine/doctrine-cache-bundle": "^1.3.3",
        "doctrine/doctrine-migrations-bundle": "^1.3.1",
        "doctrine/orm": "^2.6.1",
        "easycorp/easyadmin-bundle": "1.17.11",
        "grachevko/enum": "^0.12",
        "misd/phone-number-bundle": "^1.3.2",
        "moneyphp/money": "^3.1.3",
        "sensio/framework-extra-bundle": "^5.1.6",
        "sensiolabs/security-checker": "^4.1.8",
        "sentry/sentry-symfony": "^2.0.2",
        "symfony/asset": "^4.0.8",
        "symfony/console": "^4.0.8",
        "symfony/event-dispatcher": "^4.0.8",
        "symfony/expression-language": "^4.0.8",
        "symfony/flex": "^1.0.78",
        "symfony/form": "^4.0.8",
        "symfony/framework-bundle": "^4.0.8",
        "symfony/monolog-bundle": "^3.2.0",
        "symfony/security-bundle": "^4.0.8",
        "symfony/serializer-pack": "^1.0.1",
        "symfony/swiftmailer-bundle": "^3.2.2",
        "symfony/templating": "^4.0.8",
        "symfony/translation": "^4.0.8",
        "symfony/twig-bundle": "^4.0.8",
        "symfony/yaml": "^4.0.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.11.1",
        "lookyman/phpstan-symfony": "^0.5.0",
        "phpstan/phpstan": "^0.9.2",
        "phpstan/phpstan-doctrine": "^0.9.1",
        "phpstan/phpstan-phpunit": "^0.9.4",
        "phpunit/phpunit": "^7.1.4",
        "symfony/debug-bundle": "^4.0.8",
        "symfony/dotenv": "^4.0.8",
        "symfony/requirements-checker": "^1.0.5",
        "symfony/web-profiler-bundle": "^4.0.8",
        "symfony/browser-kit": "^4.0.8",
        "symfony/phpunit-bridge": "^4.0.8"
    },
    "scripts": {
        "auto-scripts": {
            "cache:warmup": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "src",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
